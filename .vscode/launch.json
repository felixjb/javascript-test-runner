{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Run",
      "type": "extensionHost",
      "request": "launch",
      "preLaunchTask": "Build & Watch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "outFiles": ["${workspaceFolder}/out"],
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"]
    }
  ]
}
